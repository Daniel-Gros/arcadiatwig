{% extends 'base.html.twig' %}

{% block title %}Commentaire Habitat{% endblock %}

{% block body %}
    <h1>Commentaire Habitat</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ commentaire_habitat.id }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ commentaire_habitat.message }}</td>
            </tr>
            <tr>
                <th>Amélioration</th>
                <td>{{ commentaire_habitat.improvement ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_commentaire_habitat_crud_index') }}">reoutr à la liste</a>

    <a href="{{ path('app_commentaire_habitat_crud_edit', {'id': commentaire_habitat.id}) }}">éditer</a>

    {{ include('commentaire_habitat_crud/_delete_form.html.twig') }}
{% endblock %}
